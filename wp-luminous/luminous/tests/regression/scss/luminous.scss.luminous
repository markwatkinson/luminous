    <KEYWORD>&amp;</KEYWORD><VARIABLE>.line-no-width-3</VARIABLE> <OPERATOR>&gt;</OPERATOR> <KEYWORD>span</KEYWORD><OPERATOR>:before</OPERATOR> <OPERATOR>{</OPERATOR> <TYPE>width</TYPE><OPERATOR>:</OPERATOR> <NUMERIC>3em</NUMERIC> <KEYWORD>!important</KEYWORD><OPERATOR>;</OPERATOR> <OPERATOR>}</OPERATOR>
    <KEYWORD>&amp;</KEYWORD><VARIABLE>.line-no-width-4</VARIABLE> <OPERATOR>&gt;</OPERATOR> <KEYWORD>span</KEYWORD><OPERATOR>:before</OPERATOR> <OPERATOR>{</OPERATOR> <TYPE>width</TYPE><OPERATOR>:</OPERATOR> <NUMERIC>4em</NUMERIC> <KEYWORD>!important</KEYWORD><OPERATOR>;</OPERATOR> <OPERATOR>}</OPERATOR>
    <KEYWORD>&amp;</KEYWORD><VARIABLE>.line-no-width-5</VARIABLE> <OPERATOR>&gt;</OPERATOR> <KEYWORD>span</KEYWORD><OPERATOR>:before</OPERATOR> <OPERATOR>{</OPERATOR> <TYPE>width</TYPE><OPERATOR>:</OPERATOR> <NUMERIC>5em</NUMERIC> <KEYWORD>!important</KEYWORD><OPERATOR>;</OPERATOR> <OPERATOR>}</OPERATOR>
    <KEYWORD>&amp;</KEYWORD><VARIABLE>.line-no-width-6</VARIABLE> <OPERATOR>&gt;</OPERATOR> <KEYWORD>span</KEYWORD><OPERATOR>:before</OPERATOR> <OPERATOR>{</OPERATOR> <TYPE>width</TYPE><OPERATOR>:</OPERATOR> <NUMERIC>6em</NUMERIC> <KEYWORD>!important</KEYWORD><OPERATOR>;</OPERATOR> <OPERATOR>}</OPERATOR>
    
    <KEYWORD>&amp;</KEYWORD><VARIABLE>.line-no-hidden</VARIABLE> <OPERATOR>&gt;</OPERATOR> <KEYWORD>span</KEYWORD> <OPERATOR>{</OPERATOR>
        <TYPE>padding-left</TYPE><OPERATOR>:</OPERATOR> <NUMERIC>0</NUMERIC><OPERATOR>;</OPERATOR>
        <KEYWORD>&amp;</KEYWORD><OPERATOR>::before</OPERATOR> <OPERATOR>{</OPERATOR> <TYPE>display</TYPE><OPERATOR>:</OPERATOR> <VALUE>none</VALUE><OPERATOR>;</OPERATOR> <OPERATOR>}</OPERATOR>
    <OPERATOR>}</OPERATOR>
    
  <OPERATOR>}</OPERATOR>
  
