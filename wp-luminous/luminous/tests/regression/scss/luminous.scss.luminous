      <TYPE>overflow</TYPE><OPERATOR>:</OPERATOR> <VALUE>visible</VALUE><OPERATOR>;</OPERATOR>
    <OPERATOR>}</OPERATOR>
  <OPERATOR>}</OPERATOR>
  <VARIABLE>.link</VARIABLE> <OPERATOR>{</OPERATOR>
