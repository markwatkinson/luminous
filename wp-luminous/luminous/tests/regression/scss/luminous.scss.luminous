    <TYPE>font-size</TYPE><OPERATOR>:</OPERATOR> <VALUE>inherit</VALUE><OPERATOR>;</OPERATOR>
    <TYPE>font-family</TYPE><OPERATOR>:</OPERATOR> <VALUE>inherit</VALUE><OPERATOR>;</OPERATOR>
