  fa_id                 <TYPE>INTEGER</TYPE>       <OPERATOR>NOT</OPERATOR> <VALUE>NULL</VALUE>  <KEYWORD>PRIMARY</KEYWORD> <KEYWORD>KEY</KEYWORD>,
