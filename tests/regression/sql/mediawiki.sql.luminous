  fa_height             <TYPE>SMALLINT</TYPE>     <OPERATOR>NOT</OPERATOR> <VALUE>NULL</VALUE>,
  fa_metadata           CLOB,
  fa_bits               <TYPE>SMALLINT</TYPE>,
