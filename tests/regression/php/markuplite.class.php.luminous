  <KEYWORD>function</KEYWORD> <USER_FUNCTION>MarkupLite</USER_FUNCTION>()
  {
    <VARIABLE>$this</VARIABLE><OPERATOR>-&gt;</OPERATOR><OO>AddHandler</OO>(<STRING>'prepare'</STRING>, <FUNCTION>array</FUNCTION>(<VARIABLE>$this</VARIABLE>, <STRING>'PreparePost'</STRING>), <NUMERIC>0</NUMERIC>);
    <VARIABLE>$this</VARIABLE><OPERATOR>-&gt;</OPERATOR><OO>AddHandler</OO>(<STRING>'code'</STRING>, <FUNCTION>array</FUNCTION>(<VARIABLE>$this</VARIABLE>, <STRING>'ParseCodeBlock'</STRING>), <NUMERIC>3</NUMERIC>);   
    <VARIABLE>$this</VARIABLE><OPERATOR>-&gt;</OPERATOR><OO>AddHandler</OO>(<STRING>'alignment'</STRING>, <FUNCTION>array</FUNCTION>(<VARIABLE>$this</VARIABLE>, <STRING>'ParseAlignment'</STRING>), <NUMERIC>5</NUMERIC>);
