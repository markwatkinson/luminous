  {
    <KEYWORD>return</KEYWORD> <FUNCTION>preg_replace_callback</FUNCTION>(<STRING>"/^([=]+)(.*?)<ESC>\\</ESC>1(.*)/m"</STRING>, 
      <FUNCTION>array</FUNCTION>(<VARIABLE>$this</VARIABLE>, <STRING>'ParseHeadersCb'</STRING>), <VARIABLE>$str</VARIABLE>);
  }

  <KEYWORD>function</KEYWORD> <USER_FUNCTION>ParseWhitespace</USER_FUNCTION>(<VARIABLE>$str</VARIABLE>)
  {
    
