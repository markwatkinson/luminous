        <TYPE>float</TYPE> cumulative_weighting <OPERATOR>=</OPERATOR> GetNodeWeighting(<NUMERIC>0</NUMERIC>, <OBJ>node</OBJ><OPERATOR>-&gt;</OPERATOR><OO>mName</OO><OPERATOR>.</OPERATOR><OO>data</OO>);
