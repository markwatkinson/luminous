    {
        <KEYWORD>for</KEYWORD> (x <OPERATOR>=</OPERATOR> <NUMERIC>1</NUMERIC>; x <OPERATOR>&lt;=</OPERATOR> <NUMERIC>20</NUMERIC>; x<OPERATOR>++</OPERATOR>)
        {
