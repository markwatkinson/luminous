        image[ri <OPERATOR>+</OPERATOR> <NUMERIC>21</NUMERIC>] <OPERATOR>=</OPERATOR> bord;
        <KEYWORD>if</KEYWORD> (lft) image[ri <OPERATOR>+</OPERATOR> <NUMERIC>1</NUMERIC>] <OPERATOR>=</OPERATOR> bord;
